{
  "manifest_version": 3,
  "name": "EasyEDA Font Injector",
  "version": "1.2",
  "permissions": [
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "https://easyeda.com/*"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "main.min.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  }
}